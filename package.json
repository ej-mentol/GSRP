{
  "name": "gsrp-electron",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "clean": "if exist dist rmdir /s /q dist && if exist dist-electron rmdir /s /q dist-electron",
    "build:dotnet": "dotnet publish data_service/GSRP.Daemon.csproj -c Release -r win-x64 --self-contained",
    "build": "npm run build:dotnet && tsc && vite build && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.gsrp.electron",
    "productName": "GSRP Client",
    "files": [
      "dist",
      "dist-electron",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "./data_service/bin/Release/net9.0-windows/win-x64/publish",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "dir"
    },
    "asar": true
  },
  "dependencies": {
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
